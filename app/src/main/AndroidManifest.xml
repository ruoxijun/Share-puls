<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.share">

    <!-- 添加网络权限 -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:networkSecurityConfig="@xml/network_security_config"
        android:allowBackup="true"
        android:icon="@mipmap/appstore"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/appstore"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/network_security_config" />

        <activity android:name=".main.addition.UserActivity" />
        <activity android:name=".main.addition.AttActivity" />
        <activity android:name=".blog.AddBlogActivity" />
        <activity android:name=".blog.BlogActivity" />
        <activity android:name=".firstchoice.RegisterActivity" />
        <activity android:name=".firstchoice.LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.MainActivity" />
    </application>

</manifest>